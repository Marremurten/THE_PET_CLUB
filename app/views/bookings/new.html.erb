<div class="container">
  <div class="text-center">
    <h1>New booking</h1>
  </div>

  <div>
    <%= simple_form_for [@pet, @booking] do |f| %>
      <%= f.input :start_date, html5: true %>
      <%= f.input :end_date, html5: true %>

      <%= f.button :submit, "Book", class: "btn-confirm" %>
    <% end %>
  </div>
</div>
