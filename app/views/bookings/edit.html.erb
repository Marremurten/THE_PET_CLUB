<div class="container">

  <div class="text-center">
    <h1>Change date</h1>
  </div>

  <div>
    <%= simple_form_for [@pet, @booking] do |f| %>
      <%= f.input :start_date, html5: true %>
      <%= f.input :end_date, html5: true %>

      <%= f.button :submit, "Change", class: "btn-post" %>
    <% end %>
  </div>
</div>
